<!--Link  MuJoCo modeling: http://www.mujoco.org/book/modeling.html-->

<mujoco model="ur5_allegro">
    <compiler angle="radian"
              inertiafromgeom="true"
              texturedir="textures/"
              meshdir="mesh/visual/"/>
    <option timestep="2e-3"
            iterations="10"
            tolerance="1e-10"
            impratio="20"/>
    <size njmax="1000" nconmax="150" nstack="30000000"/>

    <default>
        <site rgba="0.8 0.6 0.7 0.3" type="ellipsoid" group="3"/>
        <!-- <geom contype='1' conaffinity='1' condim='4'  margin="1e-3" solref=".01 1" solimp=".99 .99 .01" /> -->
        <geom contype='1' conaffinity='1' condim='4'  solref=".01 1" solimp=".99 .99 .01" />
        <joint armature="0.01" damping=".1"/>
        <default class="UR5">
            <joint damping='100'/>
<!--            <joint damping='0'/>-->
        </default>
        <default class="UR5e">
            <joint damping='45'/>
<!--            <joint damping='0'/>-->
        </default>
<!--        <default class="GRIPPER">-->
<!--            <joint damping='5'/>-->
<!--            <geom friction='1 0.5 0.5'/>-->
<!--        </default>-->
    </default>

    <visual>
        <map fogstart="3" fogend="5" znear="0.05"/>
        <quality shadowsize="2048"/>
<!--        <scale framelength="0.1"-->
<!--               framewidth="0.005"        -->
        <scale framelength="0.3"
               framewidth="0.01"
               contactwidth="0.1"
               contactheight="0.005"
               forcewidth="0.01"
               />
